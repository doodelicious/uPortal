<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<portlet-definition version="4.0" xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition https://source.jasig.org/schemas/uportal/io/portlet-definition/portlet-definition-4.0.xsd" xmlns:ns2="https://source.jasig.org/schemas/uportal" xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition" xmlns:ns4="https://source.jasig.org/schemas/uportal/io/permission-owner" xmlns:ns3="https://source.jasig.org/schemas/uportal/io/event-aggregation" xmlns:ns5="https://source.jasig.org/schemas/uportal/io/user" xmlns:ns6="https://source.jasig.org/schemas/uportal/io/stylesheet-descriptor" xmlns:ns7="https://source.jasig.org/schemas/uportal/io/portlet-type" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns8="https://source.jasig.org/schemas/uportal/io/subscribed-fragment">
    <title>Food Network</title>
    <name>Food Network</name>
    <fname>foodnetwork</fname>
    <desc>Food Network is committed to sharing the art of cuisine and cooking through news, recipies, and television. (foodnetwork.com)</desc>
    <type>Inline Frame</type>
    <timeout>5000</timeout>
    <portlet-descriptor>
        <ns2:isFramework>true</ns2:isFramework>
        <ns2:portletName>IFrame</ns2:portletName>
    </portlet-descriptor>
    <category>Entertainment</category>
    <group>Everyone</group>
    <parameter>
        <name>alternate</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>blockImpersonation</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>disableDynamicTitle</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>editable</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasAbout</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasHelp</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hideFromMobile</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>highlight</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>iconUrl</name>
        <value>/uPortal/media/skins/universality/common/byu-icons/foodnetwork.png</value>
    </parameter>
    <parameter>
        <name>mobileIconUrl</name>
        <value>/uPortal/media/skins/universality/common/byu-icons/mobile/foodnetwork.png</value>
    </parameter>
    <parameter>
        <name>showChrome</name>
        <value>true</value>
    </parameter>
    <portlet-preference>
        <name>frameBorder</name>
        <readOnly>false</readOnly>
        <value>0</value>
    </portlet-preference>
    <portlet-preference>
        <name>height</name>
        <readOnly>false</readOnly>
        <value>325</value>
    </portlet-preference>
    <portlet-preference>
        <name>url</name>
        <readOnly>false</readOnly>
        <value>http://m.foodnetwork.com</value>
    </portlet-preference>
    <portlet-preference>
        <name>width</name>
        <readOnly>false</readOnly>
        <value>100%</value>
    </portlet-preference>
</portlet-definition>
