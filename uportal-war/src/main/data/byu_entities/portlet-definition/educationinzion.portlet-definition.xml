<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<portlet-definition version="4.0" xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition https://source.jasig.org/schemas/uportal/io/portlet-definition/portlet-definition-4.0.xsd" xmlns:ns2="https://source.jasig.org/schemas/uportal" xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition" xmlns:ns4="https://source.jasig.org/schemas/uportal/io/permission-owner" xmlns:ns3="https://source.jasig.org/schemas/uportal/io/event-aggregation" xmlns:ns5="https://source.jasig.org/schemas/uportal/io/user" xmlns:ns6="https://source.jasig.org/schemas/uportal/io/stylesheet-descriptor" xmlns:ns7="https://source.jasig.org/schemas/uportal/io/portlet-type" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns8="https://source.jasig.org/schemas/uportal/io/subscribed-fragment">
    <title>Education in Zion</title>
    <name>Education in Zion</name>
    <fname>educationinzion</fname>
    <desc>Education in Zion is a learning center with a multimedia exhibition in the JFSB gallery.</desc>
    <type>RSS</type>
    <timeout>10000</timeout>
    <portlet-descriptor>
        <ns2:webAppName>/NewsReaderPortlet</ns2:webAppName>
        <ns2:portletName>single-feed-news</ns2:portletName>
    </portlet-descriptor>
    <category>Academics</category>
    <group>Everyone</group>
    <parameter>
        <name>alternate</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>blockImpersonation</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>disableDynamicTitle</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>editable</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasAbout</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasHelp</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hideFromMobile</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>highlight</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>iconUrl</name>
        <value>/uPortal/media/skins/universality/common/byu-icons/education.png</value>
    </parameter>
    <parameter>
        <name>mobileIconUrl</name>
        <value>/uPortal/media/skins/universality/common/byu-icons/mobile/education.png</value>
    </parameter>
    <parameter>
        <name>showChrome</name>
        <value>true</value>
    </parameter>
    <portlet-preference>
        <name>name</name>
        <readOnly>true</readOnly>
        <value>Education in Zion</value>
    </portlet-preference>
    <portlet-preference>
        <name>summaryView</name>
        <readOnly>false</readOnly>
        <value>flyout</value>
    </portlet-preference>
    <portlet-preference>
        <name>url</name>
        <readOnly>true</readOnly>
        <value>http://lib.byu.edu/sites/educationinzion/feed/</value>
    </portlet-preference>
</portlet-definition>
